{
    "rules": {
        "prettier/prettier": "error"
    },
    "globals": {
        "SharedArrayBuffer": "readonly",
        "Atomics": "readonly"
    },
    "extends": [
        "standard",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "env": {
        "node": true,
        "es6": true,
        "commonjs": true
    },
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    }
}